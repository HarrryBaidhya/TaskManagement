@model IEnumerable<TaskManagement.Models.EmployeeModel>

@{
    Layout = "_Layout";
    // var title = "Display";
    var datacheck = Model;
    // ViewData["Title"] = title;
}

<table class="table">
    <thead>
        <tr>
            <@* th>
                @Html.DisplayNameFor(model => model.Id)
            </th> *@
            <th>
                @Html.DisplayNameFor(model => model.EmployeeName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmployeeAge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmployeeSalary)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProfileImage)

            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.EmployeeName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmployeeAge)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmployeeSalary)
                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.ProfileImage)
                </td>
                <td>
                </td>
            </tr>

        }

    </tbody>
</table>



